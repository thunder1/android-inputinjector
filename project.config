[project]
	description = Android library that enables developers to inject touch and key event sin their own and external apps. Support for Android 2.3 and forward.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/thunder1
[access "refs/for/refs/*"]
	read = group user/thunder1
	push = group user/thunder1
	label-Code-Review = -2..+2 group user/thunder1
	label-Verified = -1..+1 group user/thunder1
	rebase = group user/thunder1
	removeReviewer = group user/thunder1
	submit = group user/thunder1
[access "refs/heads/*"]
	read = group user/thunder1
	create = group user/thunder1
	push = +force group user/thunder1
	pushMerge = group user/thunder1
[access "refs/tags/*"]
	push = group user/thunder1
	push = +force group user/thunder1
